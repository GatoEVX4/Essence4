                            <BlurEffect Radius="150"/>
                        </Ellipse.Effect>
                        <Ellipse.RenderTransform>
                            <TransformGroup>
                                <RotateTransform x:Name="fff3" Angle="-3266.744"/>
                            </TransformGroup>
