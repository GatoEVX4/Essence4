    <Window.Resources>
        <ResourceDictionary>

            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme
                    BaseTheme="Dark"
                    PrimaryColor="Red"
                    SecondaryColor="Teal" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Button.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style
                x:Key="{x:Type Button}"
                BasedOn="{StaticResource MaterialDesignRaisedButton}"
                TargetType="{x:Type Button}" />



            <Storyboard
                x:Key="GradientAnimation"
                AutoReverse="True"
                RepeatBehavior="Forever">
                <DoubleAnimation
                    Storyboard.TargetName="db3bdy32bd"
                    Storyboard.TargetProperty="Offset"
                    From="0.02"
                    To="1.5"
                    Duration="0:0:3" />
            </Storyboard>

            <!--<BitmapImage x:Key="masterImage" DecodePixelWidth="200"
                     UriSource="/ImageResources/Ads+Premium/Resellers/serenity.png" />

            <FormatConvertedBitmap x:Key="convertFormatImage"
                               Source="{StaticResource masterImage}"
                               DestinationFormat="Gray32Float" />-->
        </ResourceDictionary>
    </Window.Resources>

    <Window.Triggers>

        <EventTrigger RoutedEvent="FrameworkElement.Unloaded">
            <BeginStoryboard>
                <Storyboard x:Name="FormFadeOut" Completed="CloseWindow">
                    <DoubleAnimation
                        x:Name="FormFadeOutAnimation"
                        AutoReverse="False"
                        RepeatBehavior="1x"
                        Storyboard.TargetName="MainWin"
                        Storyboard.TargetProperty="(UIElement.Opacity)"
                        From="1.0"
                        To="0.0"
                        Duration="0:0:0.5" />
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>

    </Window.Triggers>




