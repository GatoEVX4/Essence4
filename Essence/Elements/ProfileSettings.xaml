<Window x:Class="Essence.Elements.ProfileSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Essence.Elements"
        mc:Ignorable="d"
        Title="ProfileSettings"
        AllowsTransparency="True"
        ShowInTaskbar="False"
        WindowStyle="None" Background="Transparent" Foreground="{x:Null}">

    <Grid x:Name="UserManagementGrid">
        <Border PreviewMouseDown="UserManagementIcon_PreviewMouseDown" Background="#02000000"/>
        <Border x:Name="UserManagementBorder" Loaded="UserManagementBorder_Loaded" Background="#FF141414" Margin="0,25,0,26" CornerRadius="5" BorderBrush="#FF282828" BorderThickness="1" Width="330" HorizontalAlignment="Center">
            <Grid>
                <TextBlock x:Name="sby2bsyh2bysbn" Text="Account Management" VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Center" TextAlignment="Center" Margin="0,10,0,0"/>
                <Image x:Name="CloseBtn3" Height="18" VerticalAlignment="Top" Source="/ImageResources/MainExecution/CloseWindowIcon.png" OpacityMask="#FF454545" Opacity="0.9" Cursor="Hand" HorizontalAlignment="Right" Width="18" PreviewMouseDown="UserManagementIcon_PreviewMouseDown" Margin="0,10,10,0" RenderOptions.BitmapScalingMode="HighQuality">
                    <Image.Style>
                        <Style TargetType="{x:Type Image}">
                            <Style.Triggers>
                                <EventTrigger RoutedEvent="UIElement.MouseEnter">
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetProperty="Opacity" From="0.6" To="1" BeginTime="0:0:0" Duration="0:0:0.2" />
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                                <EventTrigger RoutedEvent="UIElement.MouseLeave">
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetProperty="Opacity" From="1" To="0.6" BeginTime="0:0:0" Duration="0:0:0.2" />
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                            </Style.Triggers>
                        </Style>
                    </Image.Style>
                </Image>

                <WrapPanel VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,40,0,0">
                    <Border x:Name="accmanagementborder" Background="#FF282828" PreviewMouseDown="accmanagementborder_PreviewMouseDown" Width="30" Height="30" CornerRadius="10" BorderThickness="1" BorderBrush="#FF282828">
                        <Path Stretch="Uniform" Width="15" Height="15" Data="M12 4a4 4 0 0 1 4 4a4 4 0 0 1-4 4a4 4 0 0 1-4-4a4 4 0 0 1 4-4m0 10c4.42 0 8 1.79 8 4v2H4v-2c0-2.21 3.58-4 8-4" Fill="#FFC8C8C8" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>

                    <Border x:Name="keymanagementborder" Width="30" Height="30" PreviewMouseDown="keymanagementborder_PreviewMouseDown" CornerRadius="10" BorderThickness="1" BorderBrush="#FF282828" Margin="10,0,0,0" Background="#FF141414">
                        <Path Stretch="Uniform" Width="20" Height="20" Data="M7 15q1.25 0 2.125-.875T10 12t-.875-2.125T7 9t-2.125.875T4 12t.875 2.125T7 15m0 3q-2.5 0-4.25-1.75T1 12t1.75-4.25T7 6q2.025 0 3.538 1.15T12.65 10h8.375L23 11.975l-3.5 4L17 14l-2 2l-2-2h-.35q-.625 1.8-2.175 2.9T7 18" Fill="#FFC8C8C8"/>
                    </Border>

                    <Border x:Name="devicesmanagementborder" Width="30" Height="30" PreviewMouseDown="devicesmanagementborder_PreviewMouseDown" CornerRadius="10" BorderThickness="1" BorderBrush="#FF282828" Margin="10,0,0,0" Background="#FF141414">
                        <Path Stretch="Uniform" Width="18" Height="20" Data="M4 6.25A2.25 2.25 0 0 1 6.25 4h11.5A2.25 2.25 0 0 1 20 6.25v7.5A2.25 2.25 0 0 1 17.75 16H6.25A2.25 2.25 0 0 1 4 13.75zM2.75 17.5a.75.75 0 0 0 0 1.5h18.5a.75.75 0 0 0 0-1.5z" Fill="#FFC8C8C8" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </WrapPanel>

                <Grid>
                    <ScrollViewer  Margin="3,85,3,3" x:Name="accmanagementviwer">
                        <WrapPanel Margin="30,0,10,0">

                            <Border HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF1E1E1E" CornerRadius="10" Width="264">
                                <Grid Margin="10">
                                    <Border Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50">
                                        <Border.Clip>
                                            <RectangleGeometry RadiusX="10" RadiusY="10" Rect="0,0,50,50"/>
                                        </Border.Clip>

                                        <Image x:Name="User_Img3" Source="/ImageResources/Settings/user.png" RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased" />
                                    </Border>

                                    <TextBlock x:Name="f3f3nfy3uf3f" VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" TextAlignment="Center" Margin="60,8,0,0" Text="M4A1"/>
                                    <TextBlock x:Name="dn32un3cr329c8n3" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="60,0,0,8" Text="user login" FontSize="11" Foreground="#FF969696"/>
                                </Grid>
                            </Border>

                            <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Margin="10,20,0,0" Text="User Role" Width="244"/>
                            <TextBlock x:Name="dn3u2ndu2d3n" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="10,0,0,0" Text="Owner" FontSize="11" Foreground="#FF969696"/>

                            <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Margin="10,20,0,0" Text="Premium" Width="244"/>
                            <TextBlock x:Name="dudu3nud3nud" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="10,0,0,0" Text="No" FontSize="11" Foreground="#FF969696"/>

                            <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Margin="10,20,0,0" Text="Invites" Width="244"/>
                            <TextBlock x:Name="invitestxt" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="10,0,0,0" Text="0" FontSize="11" Foreground="#FF969696"/>
                            <Grid HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Width="246" Margin="10,20,0,0">


                                <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Text="Invite ID"/>
                                <TextBlock x:Name="invitationIDtxt" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" Text="AJ6O0" FontSize="11" Foreground="#FF969696"/>

                            </Grid>

                            <Button x:Name="CreateAccount" BorderThickness="0" Padding="0" materialDesign:ButtonAssist.CornerRadius="5"  Style="{StaticResource MaterialDesignFlatDarkBgButton}" VerticalAlignment="Bottom" Height="35" Background="#FF282828" Foreground="#FFE6E6E6" FontFamily="Segoe UI Semibold" Margin="32,20,0,10" Width="200" FontSize="14">
                                <TextBlock Style="{x:Null}" Text="Log Out" Foreground="#FFC81414" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="16" HorizontalAlignment="Center" FontWeight="Bold"/>
                            </Button>
                        </WrapPanel>
                    </ScrollViewer>

                    <ScrollViewer x:Name="keymanagementviwer" Margin="3,85,3,3" Background="#FF141414" Visibility="Collapsed">
                        <WrapPanel Margin="30,0,10,0">

                            <Border HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF1E1E1E" CornerRadius="10" Width="264">
                                <Grid Margin="10">
                                    <Border Height="50" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50">
                                        <Border.Clip>
                                            <RectangleGeometry RadiusX="10" RadiusY="10" Rect="0,0,50,50"/>
                                        </Border.Clip>

                                        <Image x:Name="User_Img34" Source="/ImageResources/Ads+Premium/noto-v1--crown.png" RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased" />
                                    </Border>

                                    <TextBlock VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" TextAlignment="Center" Margin="60,8,0,0" Text="Subscription"/>
                                    <TextBlock x:Name="subscription_typetxt" VerticalAlignment="Bottom" TextWrapping="Wrap" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="60,0,0,8" Text="Free" FontSize="11" Foreground="#FF969696"/>
                                </Grid>
                            </Border>

                            <TextBlock VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Margin="10,20,0,0" Text="Expiration" Width="244"/>
                            <TextBlock x:Name="key_expirationtxt" VerticalAlignment="Bottom" TextWrapping="Wrap" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="10,0,0,0" Text="---" FontSize="11" Foreground="#FF969696"/>

                            <Grid x:Name="sellerinfoborder" Margin="10,20,0,0" Width="246" Height="33">
                                <Border Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30">
                                    <Border.Clip>
                                        <RectangleGeometry RadiusX="10" RadiusY="10" Rect="0,0,30,30"/>
                                    </Border.Clip>

                                    <Image x:Name="User_Img364" Source="/Elements/M4A1.png" RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased" Visibility="Hidden" />
                                </Border>

                                <TextBlock VerticalAlignment="Top" TextWrapping="Wrap" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" TextAlignment="Center" Text="Key Provider"/>
                                <TextBlock x:Name="key_sellertxt" VerticalAlignment="Bottom" TextWrapping="Wrap" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Text="M4A1" FontSize="11" Foreground="#FF969696"/>
                            </Grid>
                            <Grid x:Name="creationdateborder" HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Margin="10,20,0,0">

                                <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Text="Creation Date" Width="244"/>
                                <TextBlock x:Name="key_creation_date" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" Text="26/01/2025" FontSize="11" Foreground="#FF969696"/>
                            </Grid>
                            <Grid x:Name="sellersuportserverborder" HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Width="246" Margin="10,20,0,0">

                                <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Text="Seller Support Server"/>

                                <TextBlock x:Name="sellerservertxt" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" Text="discord.gg/getessence" FontSize="11" Foreground="#FF969696"/>

                                <Button x:Name="copykeysupportserver" Padding="0" BorderThickness="0" materialDesign:ButtonAssist.CornerRadius="4" Click="copykeysupportserver_Click"  Height="25" Width="25" HorizontalAlignment="Right" VerticalAlignment="Center" Background="#19000000" Style="{StaticResource MaterialDesignFlatDarkBgButton}">
                                    <Image Source="/ImageResources/ScriptHub/copiar.png" Width="16" Height="16" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                </Button>
                            </Grid>
                            <Grid x:Name="premiumkeyinfoborder" HorizontalAlignment="Left" Height="44" VerticalAlignment="Top" Width="246" Margin="10,20,0,0">
                                <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" Text="Key" Width="244"/>
                                <TextBlock x:Name="premiumkeytxt" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" Margin="0,0,0,7" Text="FY66UW-KWP5L1-XCQBAY..." FontSize="10" Foreground="#FF969696"/>
                                <Button Padding="0" BorderThickness="0" materialDesign:ButtonAssist.CornerRadius="4" Height="25" Width="25" HorizontalAlignment="Right" VerticalAlignment="Center" Background="#19000000" Style="{StaticResource MaterialDesignFlatDarkBgButton}" Visibility="Hidden">
                                    <Image Source="/ImageResources/ScriptHub/copiar.png" Width="16" Height="16" RenderOptions.BitmapScalingMode="HighQuality" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                                </Button>
                            </Grid>
                        </WrapPanel>
                    </ScrollViewer>

                    <ScrollViewer x:Name="devicesmanagementviwer" Margin="3,85,3,3" Visibility="Collapsed">
                        <WrapPanel x:Name="rapedevices" Margin="30,0,10,0">
                            <Border HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF1E1E1E" CornerRadius="10" Width="264" Height="70">
                                <Grid Margin="10">
                                    <Path x:Name="dy3bdhb3db" Stretch="Uniform" Width="30" Height="30" Data="M384 96v224H64V96zM64 32C28.7 32 0 60.7 0 96v224c0 35.3 28.7 64 64 64h117.3l-10.7 32H96c-17.7 0-32 14.3-32 32s14.3 32 32 32h256c17.7 0 32-14.3 32-32s-14.3-32-32-32h-74.7l-10.7-32H384c35.3 0 64-28.7 64-64V96c0-35.3-28.7-64-64-64zm464 0c-26.5 0-48 21.5-48 48v352c0 26.5 21.5 48 48 48h64c26.5 0 48-21.5 48-48V80c0-26.5-21.5-48-48-48zm16 64h32c8.8 0 16 7.2 16 16s-7.2 16-16 16h-32c-8.8 0-16-7.2-16-16s7.2-16 16-16m-16 80c0-8.8 7.2-16 16-16h32c8.8 0 16 7.2 16 16s-7.2 16-16 16h-32c-8.8 0-16-7.2-16-16m32 160a32 32 0 1 1 0 64a32 32 0 1 1 0-64" Fill="Gainsboro" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                                    <TextBlock VerticalAlignment="Top" Foreground="Gainsboro" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" FontSize="16"  HorizontalAlignment="Left" TextAlignment="Center" Margin="60,8,0,0" Text="Windows - This PC"/>
                                    <TextBlock x:Name="dn3un3undu3nu" VerticalAlignment="Bottom" FontFamily="/Essence;component/Fonts/#Montserrat SemiBold" HorizontalAlignment="Left" TextAlignment="Center" Margin="60,0,0,8" Text="City, State" FontSize="11" Foreground="#FF969696"/>
                                </Grid>
                            </Border>
                        </WrapPanel>
                    </ScrollViewer>
                </Grid>
            </Grid>
        </Border>
    </Grid>

</Window>
