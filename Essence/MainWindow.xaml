<Window
    x:Class="Essence.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Elements="clr-namespace:Essence.Elements"
    xmlns:avalonEdit="http://icsharpcode.net/sharpdevelop/avalonedit"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:local="clr-namespace:Essence"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="MainWin"
    Title="Essence"
    Width="850"
    Height="550"
    MinWidth="740"
    MinHeight="457"
    AllowDrop="True"
    AllowsTransparency="True"
    Background="{x:Null}"
    Closing="MainWin_Closing"
    Foreground="#FF939292"
    Icon="/essence.ico"
    Loaded="MainWin_Loaded"
    SizeChanged="MainWin_SizeChanged"
    StateChanged="MainWin_StateChanged"
    Topmost="True"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">

    <Window.Resources>
        <ResourceDictionary>
            <Storyboard x:Key="HomeButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="HomeGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="HubPath15" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="HomeButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="HomeGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="HubPath15" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>


            <Storyboard x:Key="ExecutorButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="ExecutorGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="ExecutorPath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="ExecutorButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="ExecutorGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="ExecutorPath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>






            <Storyboard x:Key="HubButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="HubGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="HubPath2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="HubButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="HubGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="HubPath2" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>




            <Storyboard x:Key="ScriptsButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="ScriptsGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />


                <ColorAnimationUsingKeyFrames Storyboard.TargetName="SettingsPath4" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="ScriptsButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="ScriptsGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="SettingsPath4" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>



            <Storyboard x:Key="AIButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="AssistentGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="AIpath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="AIButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="AssistentGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="AIpath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>


            <Storyboard x:Key="SettingsButtonSelected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="SettingsGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="0.0"
                    To="1.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="SettingsPath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="SettingsButtonDeselected">
                <DoubleAnimation
                    AutoReverse="False"
                    RepeatBehavior="1x"
                    Storyboard.TargetName="SettingsGrid"
                    Storyboard.TargetProperty="(UIElement.Opacity)"
                    From="1.0"
                    To="0.0"
                    Duration="0:0:0.2" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetName="SettingsPath1" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>



            <Style x:Key="Tabs" TargetType="{x:Type TabControl}">
                <Setter Property="Padding" Value="0" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabControl}">
                            <Grid
                                x:Name="templateRoot"
                                ClipToBounds="True"
                                KeyboardNavigation.TabNavigation="Local"
                                SnapsToDevicePixels="True">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Name="ColumnDefinition0" />
                                    <ColumnDefinition x:Name="ColumnDefinition1" Width="0" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition x:Name="RowDefinition0" Height="Auto" />
                                    <RowDefinition x:Name="RowDefinition1" Height="*" />
                                </Grid.RowDefinitions>

                                <StackPanel Orientation="Horizontal">
                                    <TabPanel
                                        x:Name="headerPanel"
                                        Margin="0,2,0,0"
                                        Panel.ZIndex="1"
                                        IsItemsHost="True"
                                        KeyboardNavigation.TabIndex="1" />
                                    <Button
                                        x:Name="AddTabButton"
                                        Width="20"
                                        Height="20"
                                        Margin="2"
                                        Padding="0"
                                        Style="{StaticResource SystemButton}">
                                        <Grid>
                                            <Path
                                                Data="M0,4.5 L9,4.5"
                                                Stroke="#FFB7B7B7"
                                                StrokeThickness="1" />
                                            <Path
                                                Data="M4.5,0 L4.5,9"
                                                Stroke="#FFB7B7B7"
                                                StrokeThickness="1" />
                                        </Grid>
                                    </Button>
                                </StackPanel>

                                <Border
                                    x:Name="contentPanel"
                                    Grid.Column="0"
                                    KeyboardNavigation.DirectionalNavigation="Contained"
                                    KeyboardNavigation.TabNavigation="Local">
                                    <ContentPresenter
                                        x:Name="PART_SelectedContentHost"
                                        Margin="{TemplateBinding Padding}"
                                        ContentSource="SelectedContent"
                                        SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                </Border>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


            <!--<Style x:Key="TextEditorStyle1" TargetType="{x:Type avalonedit:TextEditor}">
                <Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.WindowTextBrushKey}}" />
                <Setter Property="Background" Value="Transparent" />
