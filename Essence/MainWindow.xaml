<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:avalonEdit="http://icsharpcode.net/sharpdevelop/avalonedit"

    Topmost="True"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:local="clr-namespace:Essence"
    xmlns:Elements="clr-namespace:Essence.Elements"
    mc:Ignorable="d"
    
    x:Class="Essence.MainWindow"
    x:Name="MainWin"
    Title="Essence"
    
    Height="550"
    MinHeight="457"
    Width="850"
    MinWidth="740"
    
    StateChanged="MainWin_StateChanged"
    SizeChanged="MainWin_SizeChanged"
    WindowStyle="None"
    AllowsTransparency="True"
        
    Foreground="#FF939292"
    
    Icon="/essence.ico"
    
    AllowDrop="True"

    Loaded="MainWin_Loaded"
    Closing="MainWin_Closing" WindowStartupLocation="CenterScreen" Background="{x:Null}">

    <Window.Resources>
        <ResourceDictionary>
            <Storyboard x:Key="HomeButtonSelected">
                <DoubleAnimation Storyboard.TargetName="HomeGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="HubPath15">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="HomeButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="HomeGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="HubPath15">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>


            <Storyboard x:Key="ExecutorButtonSelected">
                <DoubleAnimation Storyboard.TargetName="ExecutorGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ExecutorPath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="ExecutorButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="ExecutorGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="ExecutorPath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>






            <Storyboard x:Key="HubButtonSelected">
                <DoubleAnimation Storyboard.TargetName="HubGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="HubPath2">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="HubButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="HubGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="HubPath2">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>




            <Storyboard x:Key="ScriptsButtonSelected">
                <DoubleAnimation Storyboard.TargetName="ScriptsGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />


                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="SettingsPath4">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="ScriptsButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="ScriptsGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="SettingsPath4">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>



            <Storyboard x:Key="AIButtonSelected">
                <DoubleAnimation Storyboard.TargetName="AssistentGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="AIpath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="AIButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="AssistentGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="AIpath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>


            <Storyboard x:Key="SettingsButtonSelected">
                <DoubleAnimation Storyboard.TargetName="SettingsGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0.0" To="1.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="SettingsPath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#C81414" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>

            <Storyboard x:Key="SettingsButtonDeselected">
                <DoubleAnimation Storyboard.TargetName="SettingsGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.2" AutoReverse="False" RepeatBehavior="1x" />

                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="SettingsPath1">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#B6B6B6" />
                </ColorAnimationUsingKeyFrames>
            </Storyboard>



            <Style x:Key="Tabs" TargetType="{x:Type TabControl}">
                <Setter Property="Padding" Value="0" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabControl}">
                            <Grid x:Name="templateRoot" ClipToBounds="True" SnapsToDevicePixels="True" KeyboardNavigation.TabNavigation="Local">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Name="ColumnDefinition0" />
                                    <ColumnDefinition x:Name="ColumnDefinition1" Width="0" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition x:Name="RowDefinition0" Height="Auto" />
                                    <RowDefinition x:Name="RowDefinition1" Height="*" />
                                </Grid.RowDefinitions>

                                <StackPanel Orientation="Horizontal">
                                    <TabPanel x:Name="headerPanel" IsItemsHost="True" KeyboardNavigation.TabIndex="1" Panel.ZIndex="1" Margin="0 2 0 0" />
                                    <Button x:Name="AddTabButton" Width="20" Height="20" Margin="2" Padding="0" Style="{StaticResource SystemButton}">
                                        <Grid>
                                            <Path Stroke="#FFB7B7B7" StrokeThickness="1" Data="M0,4.5 L9,4.5"  />
                                            <Path Stroke="#FFB7B7B7" StrokeThickness="1" Data="M4.5,0 L4.5,9" />
                                        </Grid>
                                    </Button>
                                </StackPanel>

                                <Border x:Name="contentPanel" Grid.Column="0" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.TabNavigation="Local">
                                    <ContentPresenter x:Name="PART_SelectedContentHost" ContentSource="SelectedContent" Margin="{TemplateBinding Padding}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                </Border>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


            <!--<Style x:Key="TextEditorStyle1" TargetType="{x:Type avalonedit:TextEditor}">
                <Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.WindowTextBrushKey}}" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="FlowDirection" Value="LeftToRight" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type avalonedit:TextEditor}">
                            <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="Transparent">
                                <ScrollViewer Name="PART_ScrollViewer" CanContentScroll="True" Focusable="False" HorizontalContentAlignment="Left" HorizontalScrollBarVisibility="{TemplateBinding HorizontalScrollBarVisibility}" Padding="0" VerticalScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}" VerticalContentAlignment="Top" Content="{Binding TextArea, RelativeSource={RelativeSource TemplatedParent}}" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="avalonedit:TextEditor.WordWrap" Value="True">
                                    <Setter TargetName="PART_ScrollViewer" Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>-->



            <SolidColorBrush x:Key="Button.Static.Background2" Color="#00ffffff" />
            <SolidColorBrush x:Key="Button.Static.Border2" Color="#FF707070" />
            <SolidColorBrush x:Key="Button.Disabled.Background2" Color="#ff181818" />
            <SolidColorBrush x:Key="Button.Disabled.Border2" Color="#FFADB2B5" />
            <SolidColorBrush x:Key="Button.Disabled.Foreground2" Color="#FF838383" />
            <Style x:Key="ButtonStyle2" TargetType="{x:Type Button}">
                <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}" />
                <Setter Property="Background" Value="{StaticResource Button.Static.Background}" />
                <Setter Property="BorderBrush" Value="{StaticResource Button.Static.Border}" />
                <Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="Padding" Value="1" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border x:Name="border" CornerRadius="3" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                                <ContentPresenter x:Name="contentPresenter" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsDefaulted" Value="True">
                                    <Setter TargetName="border" Value="{StaticResource {x:Static SystemColors.HighlightBrushKey}}" Property="BorderBrush" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="#4CB6B6B6" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter TargetName="border" Property="Background" Value="#4CB6B6B6" />
                                </Trigger>
                                <Trigger Property="IsEnabled" Value="False">
                                    <Setter TargetName="border" Property="Background" Value="{StaticResource Button.Disabled.Background}" />
                                    <Setter TargetName="border" Property="BorderBrush" Value="{StaticResource Button.Disabled.Border}" />
                                    <Setter TargetName="contentPresenter" Property="TextElement.Foreground" Value="{StaticResource Button.Disabled.Foreground}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>



            <Style x:Key="Tab2" TargetType="{x:Type TabItem}">
                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                <Setter Property="Foreground" Value="Gainsboro" />
                <Setter Property="VerticalContentAlignment" Value="Stretch" />
                <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                <Setter Property="Width" Value="100" />
                <Setter Property="Height" Value="30" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabItem}">
                            <Grid x:Name="templateRoot" SnapsToDevicePixels="True" Height="30">
                                <Border x:Name="mainBorder" CornerRadius="5" Background="#7F000000" Margin="0, 0, 5, 0" BorderBrush="#FF3C3C3C" BorderThickness="1"/>
                                <StackPanel Orientation="Horizontal" Margin="8,0,0,0">
                                    <Image RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased" Source="/ImageResources/MainExecution/mdi--language-lua.png" Width="15" Height="15" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="0,0,5,0" Visibility="Collapsed"/>
                                    <TextBox 
                            x:Name="titletxt"
                            Text="{Binding Header, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                            Background="Transparent" 
                            BorderThickness="0"
                            Style="{x:Null}"                                       
                            Foreground="{TemplateBinding Foreground}" 
                            VerticalAlignment="Center" 
                            HorizontalAlignment="Left" 
                            Width="Auto" 
                            LostFocus="TitleTextBox_LostFocus"
                            KeyDown="TitleTextBox_KeyDown"
                            TextChanged="titletxt_TextChanged"
                            Padding="0" SelectionBrush="#7FC81414" CaretBrush="#FFF0F0F0" 
                        />
                                    <Button x:Name="CloseButton" Margin="5 3 0 3" Padding="0" Height="18" Width="18" Style="{StaticResource ButtonStyle2}" Background="{x:Null}" BorderBrush="{x:Null}">
                                        <Grid>
                                            <Path Stroke="#FFB7B7B7" StrokeThickness="1" Data="M0,0 L8,8" />
                                            <Path Stroke="#FFB7B7B7" StrokeThickness="1" Data="M0,8 L8,0" />
                                        </Grid>
                                    </Button>
                                </StackPanel>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="mainBorder" Property="Background" Value="#2CB6B6B6" />
                                </Trigger>
                                <MultiDataTrigger>
                                    <MultiDataTrigger.Conditions>
                                        <Condition Value="true" Binding="{Binding IsSelected, RelativeSource={RelativeSource Mode=Self}}" />
                                        <Condition Value="Top" Binding="{Binding TabStripPlacement, RelativeSource={RelativeSource AncestorType={x:Type TabControl}}}" />
                                    </MultiDataTrigger.Conditions>
                                    <Setter Property="Panel.ZIndex" Value="1" />
                                    <Setter TargetName="mainBorder" Property="Background" Value="#FF303030" />
                                    <Setter TargetName="titletxt" Property="IsEnabled" Value="True" />
                                </MultiDataTrigger>

                                <DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource Mode=Self}}" Value="False">
                                    <Setter TargetName="titletxt" Property="IsEnabled" Value="False" />
                                </DataTrigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>




            <Style x:Key="TabHost" TargetType="{x:Type TabControl}">
                <Setter Property="HorizontalContentAlignment" Value="Center" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TabControl}">
                            <Grid x:Name="templateRoot" SnapsToDevicePixels="True" KeyboardNavigation.TabNavigation="Local">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Name="ColumnDefinition0" />
                                    <ColumnDefinition x:Name="ColumnDefinition1" Width="0" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition x:Name="RowDefinition0" Height="Auto" />
                                    <RowDefinition x:Name="RowDefinition1" Height="*" />
                                </Grid.RowDefinitions>
                                <Border Height="40"/>
                                <StackPanel Orientation="Horizontal">
                                    <TabPanel x:Name="headerPanel" IsItemsHost="True" KeyboardNavigation.TabIndex="1" Panel.ZIndex="1" Margin="5"/>
                                    <Button x:Name="AddTabButton" Width="20" Height="20" Padding="0" Style="{StaticResource ButtonStyle2}" Background="#00FFFFFF" BorderBrush="#00FFFFFF" FontSize="9" FontStretch="Condensed" VerticalAlignment="Center" Margin="-2,0,0,0">
                                        <Grid>
                                            <Path Stroke="#FFB7B7B7" StrokeThickness=".7" Data="M0,4.5 L9,4.5" />
                                            <Path Stroke="#FFB7B7B7" StrokeThickness=".7" Data="M4.5,0 L4.5,9" />
                                        </Grid>
                                    </Button>
                                </StackPanel>
                                <Border x:Name="contentPanel" CornerRadius="3" BorderThickness="0" KeyboardNavigation.DirectionalNavigation="Contained" Grid.Row="1" KeyboardNavigation.TabIndex="2" KeyboardNavigation.TabNavigation="Local">
                                    <ContentPresenter x:Name="PART_SelectedContentHost" ContentSource="SelectedContent"/>
                                </Border>
                            </Grid>

                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>






            <SolidColorBrush x:Key="TextBox.Static.Border2" Color="#FFABAdB3" />
            <SolidColorBrush x:Key="TextBox.MouseOver.Border2" Color="#FF7EB4EA" />
            <SolidColorBrush x:Key="TextBox.Focus.Border2" Color="#FF569DE5" />
            <Style x:Key="TextBoxStyle1" TargetType="{x:Type TextBox}">
                <Setter Property="Background" Value="{StaticResource {x:Static SystemColors.WindowBrushKey}}" />
                <Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="KeyboardNavigation.TabNavigation" Value="None" />
                <Setter Property="HorizontalContentAlignment" Value="Left" />
                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                <Setter Property="AllowDrop" Value="True" />
                <Setter Property="ScrollViewer.PanningMode" Value="VerticalFirst" />
                <Setter Property="Stylus.IsFlicksEnabled" Value="False" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TextBox}">
                            <Border x:Name="border" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                                <ScrollViewer x:Name="PART_ContentHost" Focusable="False" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsEnabled" Value="False">
                                    <Setter TargetName="border" Property="Opacity" Value="0.56" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>






            <Style x:Key="FocusVisual2">
                <Setter Property="Control.Template">
                    <Setter.Value>
                        <ControlTemplate>
                            <Rectangle Margin="2" SnapsToDevicePixels="True" Stroke="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeDashArray="1 2" StrokeThickness="1" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <SolidColorBrush x:Key="RadioButton.Static.Background" Color="#FFFFFFFF" />
            <SolidColorBrush x:Key="RadioButton.Static.Border" Color="#FF707070" />
            <Style x:Key="OptionMarkFocusVisual">
                <Setter Property="Control.Template">
                    <Setter.Value>
                        <ControlTemplate>
                            <Rectangle Margin="14,0,0,0" SnapsToDevicePixels="True" Stroke="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2" />
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <SolidColorBrush x:Key="RadioButton.Static.Glyph" Color="#FF212121" />
            <Style x:Key="CringeRadioButtonStyling" TargetType="{x:Type RadioButton}">
                <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}" />
                <Setter Property="Foreground" Value="{StaticResource {x:Static SystemColors.ControlTextBrushKey}}" />
                <Setter Property="BorderThickness" Value="0" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RadioButton}">
                            <ControlTemplate.Resources>
                                <ResourceDictionary>
                                    <Storyboard x:Key="CheckRadio"/>
                                    <Storyboard x:Key="UncheckRadio"/>
                                </ResourceDictionary>
                            </ControlTemplate.Resources>
                            <Grid x:Name="templateRoot" Background="#00FFFFFF" SnapsToDevicePixels="True">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Border x:Name="radioButtonBorder" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" CornerRadius="100" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="1,1,2,1" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Visibility="Hidden">
                                    <Grid x:Name="markGrid" Margin="2">
                                        <Ellipse x:Name="optionMark" MinWidth="6" MinHeight="6" Opacity="0" Fill="#00FFFFFF" />
                                    </Grid>
                                </Border>
                                <ContentPresenter x:Name="contentPresenter" Grid.Column="1" Focusable="False" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />

                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="HasContent" Value="True">
                                    <Setter Property="FocusVisualStyle" Value="{StaticResource OptionMarkFocusVisual}" />
                                    <Setter Property="Padding" Value="4,-1,0,0" />
                                </Trigger>
                                <Trigger Property="IsChecked" Value="true">
                                    <Trigger.ExitActions>
                                        <BeginStoryboard x:Name="UncheckRadio_BeginStoryboard" Storyboard="{StaticResource UncheckRadio}" />
                                    </Trigger.ExitActions>
                                    <Trigger.EnterActions>
                                        <BeginStoryboard Storyboard="{StaticResource CheckRadio}" />
                                    </Trigger.EnterActions>
                                    <Setter TargetName="optionMark" Property="Opacity" Value="1" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>


            <Style x:Key="TextBox2" TargetType="{x:Type TextBox}">
                <Setter Property="SnapsToDevicePixels" Value="true"/>
                <Setter Property="OverridesDefaultStyle" Value="true"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type TextBox}">
                            <Border x:Name="BackgroundBorder" Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1" CornerRadius="4,4,4,4">
                                <ScrollViewer x:Name="PART_ContentHost" Margin="8,6,8,6"/>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>



        </ResourceDictionary>
    </Window.Resources>

    <Window.Triggers>

        <EventTrigger RoutedEvent="FrameworkElement.Unloaded">
            <BeginStoryboard>
                <Storyboard x:Name="FormFadeOut" Completed="CloseWindow">
                    <DoubleAnimation x:Name="FormFadeOutAnimation" Storyboard.TargetName="MainWin" Storyboard.TargetProperty="(UIElement.Opacity)" From="1.0" To="0.0" Duration="0:0:0.3" AutoReverse="False" RepeatBehavior="1x" />
                </Storyboard>
            </BeginStoryboard>
        </EventTrigger>

    </Window.Triggers>

    <Border x:Name="WindowBorder" Margin="0">
        <Grid x:Name="MainGrid">
            <Border x:Name="Loading_Executor_Border" local:CreationLogger.EnableLogging="True" Visibility="Hidden"/>

            <Border x:Name="executor" Background="#FF050505" BorderBrush="#FF3C3C3C" BorderThickness="1">
                <Border.Effect>
                    <BlurEffect x:Name="executorblur" Radius="0"/>
                </Border.Effect>
                <Grid>
                    <Grid.Triggers>
                        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="MainGrid" Storyboard.TargetProperty="(UIElement.Opacity)" From="0" To="1" Duration="0:0:2" AutoReverse="False" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Grid.Triggers>

                    <Image x:Name="ExecutorBk" Opacity="0.4" RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased" Stretch="UniformToFill" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Border x:Name="ExecutorVBk" Opacity="0.4"/>
                    <Canvas x:Name="RainCanvas"/>

                    <Border x:Name="Loading_Home" local:CreationLogger.EnableLogging="True" d:IsHidden="True" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Hidden"/>
                    <Grid x:Name="HomeGrid" Margin="60,40,0,0">

                        <Grid  Margin="0,0,0,48">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
